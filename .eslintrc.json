{
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "project": "./tsconfig.json",
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "prettier"],
  "globals": {
    "d3": "readonly"
  },
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}
